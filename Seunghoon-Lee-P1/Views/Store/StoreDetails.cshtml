@model Seunghoon_Lee_P1.Models.DomainModels.Store

@{
    ViewData["Title"] = " | Store";
}

<h1>Store</h1>

<table class="table table-bordered table-sm">
    <thead>
        <tr class="thead-dark">
            <th>@Model.Name</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>Email</td>
            <td>@Model.Email</td>
        </tr>
        <tr>
            <td>Phone Number</td>
            <td>@Model.PhoneNumber</td>
        </tr>
        <tr>
            <td>Street</td>
            <td>@Model.Street</td>
        </tr>
        <tr>
            <td>City</td>
            <td>@Model.City</td>
        </tr>
        <tr>
            <td>State</td>
            <td>@Model.StateId</td>
        </tr>
    </tbody>
</table>
